{
  "compilerOptions": {
    "outDir": "dist",
    "target": "ES6",
    "module": "ESNext",
    "moduleResolution": "Node",
    "declaration": true,
    "sourceMap": true,
    "jsx": "react",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true
  },
  "exclude": ["node_modules",
  "src/components/altair/*",
  "src/App.tsx"],

  "include": ["src"]
}
